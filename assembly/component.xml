<component>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/docway-fcs/lib</outputDirectory>
		</dependencySet>
	</dependencySets>
	<fileSets>
		<fileSet>
			<directory>src/main/resources</directory>
			<outputDirectory>docway-fcs/classes</outputDirectory>
			<includes>
				<include>*.properties</include>
				<include>*.xml</include>
			</includes>
			<filtered>true</filtered>
		</fileSet>
		<fileSet>
			<directory>target/dependency-resources</directory>
			<outputDirectory>docway-fcs/classes</outputDirectory>
			<includes>
				<include>*.properties</include>
				<include>*.ccf</include>
				<include>*.xml</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>target/dependency-resources/org/apache/tika/parser</directory>
			<outputDirectory>docway-fcs/classes/org/apache/tika/parser</outputDirectory>
			<includes>
				<include>**/*.properties</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>target</directory>
			<outputDirectory>docway-fcs/lib</outputDirectory>
			<excludes>
				<exclude>*sources.jar</exclude>
				<exclude>*javadoc.jar</exclude>
				<exclude>docway-fcs.jar</exclude>
			</excludes>
			<includes>
				<include>*.jar</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>appContent/logs</directory>
			<outputDirectory>docway-fcs/logs</outputDirectory>
			<includes>
				<include>**/*</include>
			</includes>
		</fileSet>
		<fileSet>
			<outputDirectory>docway-fcs</outputDirectory>
			<includes>
				<include>INSTALL.md</include>
			</includes>
		</fileSet>
	</fileSets>
</component>